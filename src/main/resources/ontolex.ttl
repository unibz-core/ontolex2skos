                  rdfs:isDefinedBy <http://www.w3.org/ns/lemon/ontolex> ;
                  rdfs:label "Referenz"@de ,
                             "reference"@en ,
                             "referencia"@es ,
                             "referens"@sv ,
                             "referință"@ro ,
                             "referntie"@nl ,
                             "referência"@pt ,
                             "riferimento"@it ,
                             "référence"@fr ,
                             "verwysing"@af ,
                             "референция"@ru ;
                  vs:term_status "stable" .


