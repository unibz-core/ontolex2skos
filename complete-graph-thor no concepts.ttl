thor:ontolexLexicalSense_aanbieder_1
        rdf:type                        ontolex:LexicalSense , rdfs:Resource , owl:Thing , _:b12 , _:b3 , _:b4 , semiotics:Meaning , _:b11 ;
        lexinfo:domain                  thor:sociaal_domein ;
